{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "react-admin/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/react-admin/dist/(.*)",
      "dest": "/react-admin/dist/$1"
    },
    {
      "src": "/(.*\\.js|.*\\.css|.*\\.png|.*\\.jpg|.*\\.svg|.*\\.ico|.*\\.woff2|.*\\.ttf|.*\\.webp)",
      "dest": "/react-admin/dist/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/react-admin/dist/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "/react-admin/dist/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "/react-admin/dist/logo512.png"
    },
    {
      "src": "/(.*)",
      "dest": "/react-admin/dist/index.html"
    }
  ]
}